<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ednamall.ednamall.edna">

    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.WRITE_CONTACTS" />
    <uses-permission android:name="android.permission.internet" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.WRITE_SMS" />
    <uses-permission android:name="android.permission.SEND_SMS" />
    <uses-permission android:name="android.permission.RECEIVE_SMS" />
    <uses-permission android:name="android.permission.SET_WALLPAPER"></uses-permission>



    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.ednamall.ednamall.edna.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.ednamall.ednamall.edna.CommingActivity"
            android:label="@string/title_activity_comming"
            android:parentActivityName="com.ednamall.ednamall.edna.MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.ednamall.ednamall.edna.MainActivity" />
        </activity>
        <activity
            android:name="com.ednamall.ednamall.edna.NowshowingActivity"
            android:label="@string/title_activity_nowshowing"
            android:parentActivityName="com.ednamall.ednamall.edna.MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.ednamall.ednamall.edna.MainActivity" />
        </activity>
        <activity
            android:name="com.ednamall.ednamall.edna.DetailActivity"
            android:label="@string/title_activity_detail" />
        <activity
            android:name="com.ednamall.ednamall.edna.SevenDActivity"
            android:label="@string/title_activity_seven_d"
            android:parentActivityName="com.ednamall.ednamall.edna.MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.ednamall.ednamall.edna.MainActivity" />
        </activity>
        <activity
            android:name="com.ednamall.ednamall.edna.BongosActivity"
            android:label="@string/title_activity_bongos"
            android:parentActivityName="com.ednamall.ednamall.edna.MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.ednamall.ednamall.edna.MainActivity" />
        </activity>
        <activity
            android:name="com.ednamall.ednamall.edna.ContactActivity"
            android:label="@string/title_activity_contact"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="com.ednamall.ednamall.edna.FullScreenImageActivity"
            android:theme="@style/Theme.AppCompat.Dialog.Alert"/>
        <activity android:name="com.ednamall.ednamall.edna.AboutActivity"></activity>
    </application>

</manifest>